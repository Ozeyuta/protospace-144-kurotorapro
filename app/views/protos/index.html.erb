<main class="main">
  <div class="inner">
    <%@protos.each do |proto| %>
      <div class="content_post" style="background-image: url(<%= tweet.image %>);">
        <p><%= tweet.text %></p>
          <span class="name">
        <%= tweet.name %>
      </span>
    </div>
  <% end %>

    <%# ログインしているときは以下を表示する %>
    <% if current_user %>
      <div class="greeting"> 
        こんにちは、
        <%= link_to current_user.name, root_path, class: :greeting__link%>さん
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
    <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    </div>
  </div>
</main>
